<component type="desktop">
  <id>com.github.mdh34.quickdocs</id>
  <metadata_license>CC0</metadata_license>
  <project_license>GPL-3.0+</project_license>
  <name>quickDocs</name>
  <summary>Quickly Read Developer Docs</summary>
  <description>
    <p>A fast developer docs reader that supports Valadoc and DevDocs</p>
    <p>Includes:</p>
    <ul>
      <li>Offline support for both Valadoc and Devdocs</li>
      <li>Dark mode toggle to suit your workflow</li>
    </ul>
  </description>
  <provides>
    <binary>com.github.mdh34.quickdocs</binary>
  </provides>
  <developer_name>Matt Harris</developer_name>
  <url type="homepage">https://github.com/mdh34/quickDocs</url>
  <url type="bugtracker">https://github.com/mdh34/quickDocs/issues</url>
  <releases>
    <release version="2.2.0" date="2018-09-20">
      <description>
        <ul>
          <li>Fix some bugs with the offline package list</li>
          <li>Add an 'install all' button and alphabetical ordering to the offline package list</li>
          <li>Updated theme changer to use the new Granite ModeSwitch</li>
        </ul>
      </description>
    </release>
    <release version="2.1.1" date="2018-07-03">
      <description>
        <ul>
          <li>Juno AppCenter release</li>
        </ul>
      </description>
    </release>
    <release version="2.1.0" date="2018-06-30">
      <description>
        <ul>
          <li>Offline packages can now be easily searched through</li>
        </ul>
      </description>
    </release>
    <release version="2.0.5" date="2018-06-17">
      <description>
        <ul>
          <li>Fix a bug in opening some links</li>
        </ul>
      </description>
    </release>
    <release version="2.0.4" date="2018-06-08">
      <description>
        <ul>
          <li>Loki bugfixes</li>
        </ul>
      </description>
    </release>
    <release version="2.0.3" date="2018-05-31">
      <description>
        <ul>
          <li>Flatpak-friendly tweaks</li>
        </ul>
      </description>
    </release>
    <release version="2.0.2" date="2018-05-31">
      <description>
        <ul>
          <li>Better offline sidebar resizing</li>
          <li>Support for translations</li>
          <li>GNOME-friendly icons</li>
          <li>Misc patches</li>
        </ul>
      </description>
    </release>
    <release version="2.0.1" date="2018-04-15">
      <description>
        <ul>
          <li>Fixes offline DevDocs support - toggling the theme back and forth once should reload the cache</li>
        </ul>
      </description>
    </release>
    <release version="2.0.0" date="2018-04-10">
      <description>
        <p>Major update:</p>
        <ul>
          <li>Brand new UI for downloading offline docs natively</li>
          <li>Brand new offline docs UI that allows for searching and easier viewing</li>
        </ul>
      </description>
    </release>
    <release version="1.2.2" date="2018-03-23">
      <description>
        <ul>
          <li>Simplify UI</li>
        </ul>
      </description>
    </release>
    <release version="1.2.1" date="2018-03-09">
      <description>
        <ul>
          <li>UI Updates</li>
        </ul>
      </description>
    </release>
    <release version="1.2.0" date="2018-03-04">
      <description>
        <ul>
          <li>Added the ability to use Valadoc offline</li>
        </ul>
      </description>
    </release>
    <release version="1.1.6" date="2018-02-28">
      <description>
        <ul>
          <li>quickDocs now restores the last page you were on</li>
        </ul>
      </description>
    </release>
    <release version="1.1.5" date="2018-02-02">
      <description>
        <ul>
          <li>quickDocs now remembers its last size and location!</li>
        </ul>
      </description>
    </release>
    <release version="1.1.4" date="2018-01-30">
      <description>
        <ul>
          <li>Fixed a bug where DevDocs wouldn't work offline</li>
        </ul>
      </description>
    </release>
    <release version="1.1.3" date="2018-01-20">
      <description>
        <ul>
          <li>Minor UI tweaks</li>
        </ul>
      </description>
    </release>
    <release version="1.1.1" date="2018-01-15">
      <description>
        <ul>
          <li>New keyboard shortcut! Press ctrl+tab to switch tabs</li>
        </ul>
      </description>
    </release>
    <release version="1.1.0" date="2018-01-13">
      <description>
        <p>1.1.0</p>
        <ul>
          <li>New dark mode support!</li>
          <li>quickDocs remembers which tab you were last using</li>
        </ul>
      </description>
    </release>
    <release version="1.0.4" date="2018-01-09">
      <description>
        <p>Initial Release</p>
        <ul>
          <li>Support for Valadoc</li>
          <li>Support for DevDocs</li>
          <li>New Icon!</li>
          <li>Fix some AppCenter issues</li>
        </ul>
      </description>
    </release>
  </releases>
